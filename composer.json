{
    "name": "rais/momo-suite",
    "description": "Mobile Money Payment Suite for Laravel",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Rais\\MomoSuite\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Rais",
            "email": "osumanurais@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0 || ^8.1 || ^8.2",
        "guzzlehttp/guzzle": "^7.2",
        "illuminate/support": "^8.0 || ^9.0 || ^10.0 || ^11.0",
        "illuminate/console": "^8.0 || ^9.0 || ^10.0 || ^11.0",
        "illuminate/contracts": "^8.0 || ^9.0 || ^10.0 || ^11.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rais\\MomoSuite\\MomoSuiteServiceProvider"
            ],
            "commands": [
                "Rais\\MomoSuite\\Console\\InstallCommand"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
} 